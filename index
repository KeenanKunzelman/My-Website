$(function() {
var contactpos = $("#contact").offset();
var p = $("#portf").offset();






$("#btn").on("click", function(e){
e.preventDefault();

$("html, body").animate({ scrollTop: "0px" }, 'swing');

    });

    $("#btn2").on("click", function(e){
    e.preventDefault();

    $("html, body").animate({ scrollTop: (p.top - 43) } , 'swing');

        });
